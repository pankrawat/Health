<?xml version="1.0" encoding="utf-8"?><!--Text Only News Feed-->
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff"
    android:layout_margin="@dimen/dph3"
    card_view:cardCornerRadius="4dp">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <com.tupelo.wellness.view.NewTextView
            android:id="@+id/feed_title"
            android:layout_marginTop="@dimen/dph15"
            android:layout_marginLeft="@dimen/dph15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginBottom="@dimen/dph10"
            android:text="Casier Lenardo"
            android:textColor="@android:color/black"
            android:textSize="@dimen/sp16" />



        <ImageView android:id="@+id/feed_media_url"
            android:layout_width="match_parent"
            android:layout_below="@+id/feed_title"
            android:layout_marginTop="@dimen/dph10"
            android:src="@drawable/pow_by_pubnub"
            android:layout_margin="@dimen/dph1"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"/>

        <com.tupelo.wellness.view.NewTextView
            android:id="@+id/feed_timestamp"
            app:font="light"
            android:layout_marginLeft="@dimen/dph15"
            style="@style/timeStampNewsFeed"
            android:layout_marginBottom="@dimen/dph15"
            android:layout_marginTop="@dimen/dph10" />
     <!--   <ImageView
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_marginTop="@dimen/dph10"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dph10"
            android:layout_below="@+id/feed_media_url"
            android:layout_alignParentRight="true"
            android:src="@mipmap/icon"
            />
-->

    </LinearLayout>
</android.support.v7.widget.CardView>